{
  "name": "kurtney-portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm-run-all build:*",
    "build:vite": "vite build",
    "build:scss": "npm run compile:scss && npm run prefix:css && npm run compress:css",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "compile:scss": "npx node-sass src/sass/main.scss src/css/style.css",
    "prefix:css": "npx postcss --use autoprefixer -b 'last 10 versions' src/css/style.css -o src/css/style.css",
    "compress:css": "npx node-sass src/css/style.css src/css/style.css --output-style compressed"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "node-sass": "^9.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^11.0.0",
    "sass": "^1.69.7",
    "vite": "^5.0.8"
  }
}
